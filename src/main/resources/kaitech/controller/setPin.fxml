  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="192.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="192.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="192.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Text fill="#649664" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter the new pin" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.valignment="BOTTOM">
